<?php
class Ak_Cis_Block_Adminhtml_Grid extends Mage_Adminhtml_Block_Widget_Grid_Container
{
    public function __construct()
    {
     //where is the controller
     $this->_controller = 'adminhtml_grid';
     $this->_blockGroup = 'cis_grid';
     //text in the admin header
     $this->_headerText = 'Attribute Manage';
     //value of the add button
     $this->_addButtonLabel = 'Add Fields';
     parent::__construct();
     }
}
